<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="app.consult.witczak.jakub.com.concultapp.panel.student.tutors.find.fragment.details.TutorDetailsFragment">

    <TextView
        android:id="@+id/tutor_first_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toTopOf="@+id/tutor_last_name"
        app:layout_constraintTop_toTopOf="parent"
        tools:text="first name" />

    <TextView
        android:id="@+id/tutor_last_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@+id/tutor_first_name"
        tools:text="last name" />
</android.support.constraint.ConstraintLayout>
